{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-config-airbnb",
    "tslint-plugin-prettier",
    "tslint-config-prettier"
  ],
  "jsRules": {},
  "rules": {
    "quotemark": [true, "single"],
    "variable-name": [true, "ban-keywords", "check-format", "allow-pascal-case"],
    "prettier": true
  },
  "rulesDirectory": [],
  "linterOptions": {
    "exclude": [
      "src/generated/**/*",
      "node_modules/**/*.ts"
    ]
  }
}
