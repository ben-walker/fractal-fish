{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-config-airbnb"
  ],
  "jsRules": {},
  "rules": {
    "quotemark": [true, "single"],
    "variable-name": [true, "ban-keywords", "check-format", "allow-pascal-case"]
  },
  "rulesDirectory": [],
  "linterOptions": {
    "exclude": ["src/generated/**/*"]
  }
}
